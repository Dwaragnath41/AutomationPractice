		{
			"name": "Get all issues created in last 24 hrs",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var resp = pm.response.json();\r",
							"var count = resp.issues.length;\r",
							"\r",
							"var currentTime = new Date().getTime();\r",
							"var timeBefore24hrs = currentTime - (24 * 60 * 60 * 1000);\r",
							"\r",
							"var issueCount = 0;\r",
							"for(var i = 0; i < count ; i++){\r",
							"if (new Date(resp.issues[i].fields.created).getTime() > timeBefore24hrs){\r",
							"    console.log(resp.issues[i].id);\r",
							"    issueCount++;\r",
							"}\r",
							"  \r",
							"}\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"console.log(issueCount);\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
